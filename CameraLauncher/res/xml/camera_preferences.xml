    <!-- SPRD: Add View pref_camera_jpeg_quality_key @{ -->
    <ListPreference
            camera:key="pref_camera_jpeg_quality_key"
            camera:defaultValue="@string/pref_camera_jpegh_quality_entry_value_super_hight"
