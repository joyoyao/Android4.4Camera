            camera:entries="@array/pref_camera_jpeg_quality_entries"
            camera:entryValues="@array/pref_camera_jpeg_quality_entry_values" />
    <!-- }@ -->

    <!-- SPRD: Add View pref_camera_sharpness_key @{ -->
     <ListPreference
        camera:key="pref_camera_sharpness_key"
        camera:defaultValue="@string/pref_entry_value_three"
        camera:title="@string/pref_camera_sharpness_title"
        camera:entries="@array/pref_camera_sharpness_entries"
        camera:entryValues="@array/pref_camera_sharpness_entry_values" />
    <!-- }@ -->

    <!-- SPRD: Add View pref_camera_saturation_key @{ -->
    <ListPreference
        camera:key="pref_camera_saturation_key"
        camera:defaultValue="@string/pref_entry_value_three"
        camera:title="@string/pref_camera_saturation_title"
        camera:entries="@array/pref_camera_saturation_entries"
        camera:entryValues="@array/pref_camera_saturation_entry_values" />
    <!-- }@ -->

    <!-- SPRD: Add View pref_freeze_frame_display_key @{ -->
    <IconListPreference
        camera:key="pref_freeze_frame_display_key"
        camera:defaultValue="@string/pref_entry_value_off"
        camera:title="@string/pref_freeze_frame_display_title"
        camera:entries="@array/pref_switch_entries"
        camera:entryValues="@array/pref_switch_entryvalues" />
    <!-- }@ -->
    <!-- SPRD: Add view pref_camera_storage_path @{ -->
    <ListPreference
        camera:key="pref_camera_storage_path"
        camera:defaultValue="@string/pref_save_storage_path_default"
        camera:title="@string/pref_camera_storage_path_title" />
    <!-- }@ -->

</PreferenceGroup>
