    <IconListPreference
            camera:key="pref_camera_antibanding_key"
            camera:defaultValue="@string/pref_camera_antibanding_entryvalue_50"
            camera:title="@string/pref_camera_antibanding_title"
            camera:singleIcon="@drawable/ic_sce"
            camera:entries="@array/pref_camera_antibanding_entries"
            camera:labelList="@array/pref_camera_antibanding_entries"
            camera:entryValues="@array/pref_camera_antibanding_entryvalues" />
    <!-- }@ -->

    <!-- SPRD: Add View pref_camera_iso_key @{ -->
    <IconListPreference
            camera:key="pref_camera_iso_key"
            camera:defaultValue="@string/pref_entry_value_auto"
            camera:title="@string/pref_camera_iso_title"
            camera:singleIcon="@drawable/ic_sce"
            camera:entries="@array/pref_camera_iso_entries"
            camera:labelList="@array/pref_camera_iso_entries"
            camera:entryValues="@array/pref_camera_iso_entryvalues" />
    <!-- }@ -->

    <!-- SPRD: Add View pref_camera_color_effect_key @{ -->
    <IconListPreference
            camera:key="pref_camera_color_effect_key"
            camera:defaultValue="@string/pref_entry_value_none"
            camera:title="@string/pref_color_effect_title"
            camera:entries="@array/pref_camera_color_effect_entries"
            camera:entryValues="@array/pref_camera_color_effect_entryvalues" />
    <!-- }@ -->

    <!-- SPRD: Add View pref_camera_video_zsl_key @{ -->
    <IconListPreference
            camera:key="pref_camera_video_zsl_key"
            camera:defaultValue="@string/pref_entry_switch_value_off"
            camera:title="@string/pref_camera_video_zsl_title"
            camera:singleIcon="@drawable/ic_sce"
            camera:entries="@array/pref_switch_entries"
            camera:labelList="@array/pref_switch_entries"
            camera:entryValues="@array/pref_switch_entryvalues" />
    <!-- @} -->

    <!--SPRD: uui camera setting start @{ -->
    <ListPreference
            camera:key="general_settings"
            camera:defaultValue="@string/general_settings"
            camera:title="@string/general_settings" />
    <ListPreference
            camera:key="advanced_settings"
            camera:defaultValue="@string/advanced_settings"
            camera:title="@string/advanced_settings" />
    <!-- uui camera setting end @} -->

    <!-- SPRD: Add View pref_camera_video_contrast_key @{ -->
    <IconListPreference
            camera:key="pref_camera_video_contrast_key"
            camera:defaultValue="@string/pref_entry_value_three"
            camera:title="@string/pref_camera_video_contrast_title"
            camera:singleIcon="@drawable/ic_sce"
            camera:entries="@array/pref_camera_video_contrast_entry_values"
            camera:labelList="@array/pref_camera_video_contrast_entry_values"
            camera:entryValues="@array/pref_camera_video_contrast_entry_values" />
    <!-- }@ -->

    <!-- SPRD: Add View pref_camera_metering_key @{ -->
    <IconListPreference
            camera:key="pref_camera_metering_key"
            camera:defaultValue="@string/pref_camera_metering_entry_value_frame_average"
            camera:title="@string/pref_camera_metering_title"
            camera:singleIcon="@drawable/ic_exposure_holo_light"
            camera:entries="@array/pref_camera_metering_entries"
            camera:labelList="@array/pref_camera_metering_entries"
            camera:entryValues="@array/pref_camera_metering_entryvalues" />
    <!-- }@ -->

    <!-- SPRD: Add View pref_camera_brightness_key @{ -->
    <ListPreference
